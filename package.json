{
  "name": "two-factor",
  "title": "Two Factor",
  "engineStrict": true,
  "engines": {
    "node": ">=16"
  },
  "homepage": "https://github.com/wordpress/two-factor",
  "scripts": {
    "env": "wp-env",
    "deploy": "grunt deploy",
    "build": "npm run readme && grunt build",
    "test": "npm-run-all test-*",
